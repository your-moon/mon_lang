extern функц хэвлэ(н тоо64) -> хоосон  {}
extern функц унш() -> тоо64  {}

// Рекурсив аргаар Фибоначчи тооцоолох
функц фибоначчиРекурсив(н тоо64) -> тоо64 {
    хэрэв н <= 1 бол {
        буц н;
    }
    буц фибоначчиРекурсив(н - 1) + фибоначчиРекурсив(н - 2);
}

// Давталт ашиглан Фибоначчи тооцоолох
функц фибоначчиДавталт(н тоо64) -> тоо64 {
    хэрэв н <= 1 бол {
        буц н;
    }

    зарла өмнөх: тоо64 = 0;
    зарла одоогийн: тоо64 = 1;
    зарла i: тоо64 = 2;

    давтах i <= н бол {
        зарла дараах: тоо64 = өмнөх + одоогийн;
        өмнөх = одоогийн;
        одоогийн = дараах;
        i = i + 1;
    }

    буц одоогийн;
}

функц үндсэн() -> тоо {
    зарла n: тоо64 = унш();

    // Рекурсив аргаар тооцоолох
    зарла рекурсивХариу: тоо64 = фибоначчиРекурсив(n);
    хэвлэ(рекурсивХариу);

    // Давталт ашиглан тооцоолох
    зарла давталтХариу: тоо64 = фибоначчиДавталт(n);
    хэвлэ(давталтХариу);

    буц 0;
}
