функц үндсэн() -> тоо {

    зарла нийлбэр:тоо = 0;
    зарла и:тоо;

    давт и бол 1..5 хүртэл {
        нийлбэр = нийлбэр + и;
    }

    давт и бол 1..10 хүртэл {
        нийлбэр = нийлбэр + и;
    }

    давт элем бол массив дуустал {
        нийлбэр = нийлбэр + элем;
    }

    буц нийлбэр; // 15 (1+2+3+4+5)
}
