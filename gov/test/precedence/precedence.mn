функц үндсэн() -> тоо {
    зарла а:тоо = 5 + 3 * 2 + 8 / 4;     // 5 + 6 + 2 = 13
    зарла б:тоо = (5 + 3) * (2 + 8) / 4;  // 8 * 10 / 4 = 20
    зарла в:тоо = 2 * 3 * 4 + 6 / 2;      // 24 + 3 = 27

    хэрэв а > б && б < в || а == 13 бол {
        зарла г:тоо = 1;
    }

    зарла д:тоо = (а + б) * (в - 5) / (2 + 1);  // (13 + 20) * (27 - 5) / 3 = 242

    хэрэв а + б * 2 > в * 3 && б > 0 || а == 13 бол {
        буц 1;  // true because a == 13
    }

    буц д;  // return 242
} 