функц үндсэн() -> тоо {
    зарла а:тоо = 10;
    зарла б:тоо = 5;
    зарла в:тоо = 2;

    // Engiin gurvalsan
    зарла г:тоо = а > б ? 1 : 0;  // 1

    // Davtsan gurvalsan
    зарла д:тоо = а > б ?
                  б > в ? 2 : 3 :
                  в > б ? 4 : 5;  // 2

    // arithmetic uildeltei gurvalsan
    зарла е:тоо = а + б > в * 5 ?
                  а * 2 :
                  б * 3;  // 20
    буц е;

    буц г + д + е;  //out: 23
}
