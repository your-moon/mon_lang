extern функц хэвлэ(н тоо64) -> хоосон  {}
extern функц хэвлэ_мөр(м мөр) -> хоосон {}
extern функц унш() -> тоо64  {}

функц үндсэн() -> тоо {
    зарла зорилтотТоо: тоо64 = 42;
    
    хэвлэ_мөр("Тоо оруулна уу (зорилтот тоо 42):");
    зарла таамаглал: тоо64 = унш();
    
    хэрэв таамаглал == зорилтотТоо бол {
        хэвлэ_мөр("Зөв таалаа!");
    }
    
    хэрэв таамаглал < зорилтотТоо бол {
        хэвлэ_мөр("Бага байна");
    }
    
    хэрэв таамаглал > зорилтотТоо бол {
        хэвлэ_мөр("Их байна");
    }
    
    // Оруулсан тоог хэвлэх
    хэвлэ_мөр("Таны оруулсан тоо:");
    хэвлэ(таамаглал);
    
    буц 0;
}
