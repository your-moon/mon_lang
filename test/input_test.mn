extern функц хэвлэ(н тоо64) -> хоосон  {}
extern функц хэвлэ_мөр(м мөр) -> хоосон {}
extern функц унш() -> тоо64  {}
extern функц унш32() -> тоо {}

функц үндсэн() -> тоо {
    хэвлэ_мөр("Тоо оруулна уу:");
    зарла оролт: тоо = унш32();
    хэвлэ_мөр("Таны оруулсан тоо:");
    хэвлэ(оролт);
    буц 0;
}
