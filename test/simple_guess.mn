extern функц хэвлэ(н тоо64) -> хоосон  {}
extern функц мөр_хэвлэх(м мөр) -> хоосон {}
extern функц унш() -> тоо64  {}

функц үндсэн() -> тоо {
    зарла зорилтотТоо: тоо64 = 42;
    
    мөр_хэвлэх("Тоо оруулна уу (зорилтот тоо 42):");
    зарла таамаглал: тоо64 = унш();
    
    хэрэв таамаглал == зорилтотТоо бол {
        мөр_хэвлэх("Зөв таалаа!");
    }
    
    хэрэв таамаглал < зорилтотТоо бол {
        мөр_хэвлэх("Бага байна");
    }
    
    хэрэв таамаглал > зорилтотТоо бол {
        мөр_хэвлэх("Их байна");
    }
    
    // Оруулсан тоог хэвлэх
    мөр_хэвлэх("Таны оруулсан тоо:");
    хэвлэ(таамаглал);
    
    буц 0;
}
